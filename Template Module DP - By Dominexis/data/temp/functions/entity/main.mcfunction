# Run function based on entity type

execute if entity @s[tag=temp.entity.type.skull       ] run function temp:entity/skull/main
execute if entity @s[tag=temp.entity.type.ball        ] run function temp:entity/ball/main
execute if entity @s[tag=temp.entity.type.racecar     ] run function temp:entity/racecar/main
execute if entity @s[tag=temp.entity.type.spider_brain] run function temp:entity/spider_brain/main